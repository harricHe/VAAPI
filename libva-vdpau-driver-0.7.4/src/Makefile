install-vdpau_drv_video_laLTLIBRARIES: $(vdpau_drv_video_la_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(vdpau_drv_video_ladir)" || $(MKDIR_P) "$(DESTDIR)$(vdpau_drv_video_ladir)"
	@list='$(vdpau_drv_video_la_LTLIBRARIES)'; test -n "$(vdpau_drv_video_ladir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(vdpau_drv_video_ladir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(vdpau_drv_video_ladir)"; \
	}

uninstall-vdpau_drv_video_laLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(vdpau_drv_video_la_LTLIBRARIES)'; test -n "$(vdpau_drv_video_ladir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(vdpau_drv_video_ladir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(vdpau_drv_video_ladir)/$$f"; \
	done

clean-vdpau_drv_video_laLTLIBRARIES:
	-test -z "$(vdpau_drv_video_la_LTLIBRARIES)" || rm -f $(vdpau_drv_video_la_LTLIBRARIES)
	@list='$(vdpau_drv_video_la_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
